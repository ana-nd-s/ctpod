{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "moduleResolution": "node",
  "esModuleInterop": true,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/components/*"],
      "@images/*": ["src/assets/images/*"],
      "@pages/*": ["src/pages/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
